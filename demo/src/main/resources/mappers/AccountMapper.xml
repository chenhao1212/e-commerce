<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.shequds.demo.mapper.AccountMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.shequds.demo.entity.Account">
        <id column="id" property="id" />
        <result column="account_name" property="accountName" />
        <result column="brank_number" property="brankNumber" />
        <result column="account_number" property="accountNumber" />
        <result column="balance" property="balance" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, account_name, brank_number, account_number, balance
    </sql>

</mapper>
